{
    "manifest_version": 2,
    "name": "Getting started example",
    "description": "This extension is a filter for Jigsaw table",
    "version": "0.0.1",
    "icons": {
        "16": "on.png",
        "48": "on.png",
        "128": "on.png"
    },
    "browser_action": {
        "default_icon": "off.png"
    },
    "background": {
        "scripts": ["toggle.js"] 
    }, 
    "permissions": [
        "activeTab", "<all_urls>"
    ]
}
