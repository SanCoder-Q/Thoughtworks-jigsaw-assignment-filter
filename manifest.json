{
    "manifest_version": 2,
    "name": "Getting started example",
    "description": "This extension is a filter for Jigsaw table",
    "version": "0.0.1",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab", "<all_urls>"
    ]
}
